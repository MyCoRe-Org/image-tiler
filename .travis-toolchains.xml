<toolchains>
  <!-- JDK toolchains -->
  <toolchain>
    <type>jdk</type>
    <provides>
      <version>11</version>
      <vendor>oracle</vendor>
    </provides>
    <configuration>
      <jdkHome>/usr/local/lib/jvm/openjdk11</jdkHome>
    </configuration>
  </toolchain>
  <toolchain>
    <type>jdk</type>
    <provides>
      <version>12</version>
      <vendor>oracle</vendor>
    </provides>
    <configuration>
      <jdkHome>/home/travis/openjdk12</jdkHome>
    </configuration>
  </toolchain>
</toolchains>
